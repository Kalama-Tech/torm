  },
  "keywords": [
    "toonstoredb",
    "toonstore",
    "orm",
    "database",
    "toon"
  ],
  "author": "ToonStore Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ioredis": "^5.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.27",
    "typescript": "^5.9.3"
  }
}
